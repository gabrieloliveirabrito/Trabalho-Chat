<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <gradient android:angle="270"
                android:startColor="#01AA01"
                android:endColor="#10CC10"
                android:type="linear"/>
            <padding android:bottom="30dp" android:left="30dp" android:right="30dp" android:top="30dp" />
        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" android:innerRadius="50dp">
            <solid android:color="#FFFFFF" />
            <corners android:topLeftRadius="5dp" android:radius="5dp" />
            <padding android:bottom="30dp" android:left="30dp" android:right="30dp" android:top="30dp" />
        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="oval" android:innerRadius="50dp">
            <solid android:color="#003300" />
            <corners android:topLeftRadius="5dp" android:radius="5dp" />
        </shape>
    </item>
</layer-list>